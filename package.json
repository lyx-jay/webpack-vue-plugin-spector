{
  "name": "webpack-vue-plugin-inspector",
  "version": "1.0.1",
  "repository": "https://github.com/lyx-jay/webpack-vue-plugin-spector.git",
  "author": "lvyuanxin <lyxjay1005@163.com>",
  "keywords": [
    "webpack",
    "vue",
    "inspector",
    "ide"
  ],
  "scripts": {
    "build": "rm -rf dist && unbuild"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "exports": {
