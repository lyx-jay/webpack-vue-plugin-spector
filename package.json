    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@vue/compiler-core": "^3.2.47",
    "@vue/compiler-sfc": "^3.2.47",
    "express": "^4.18.2",
    "launch-editor": "^2.6.0",
    "unplugin": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.0",
    "unbuild": "^1.1.2"
  }
}
